msgid "plugins.importexport.csv.displayName"
msgstr "Dodatak za uvoz sadržaja razdvojenog tabulatorima"

msgid "plugins.importexport.csv.description"
msgstr "Uvoz podnesaka u izdavačke kuće iz podataka razdvojenih tabulatorima."

msgid "plugins.importexport.csv.cliOnly"
msgstr ""
"\n"
"\t\t<p>Ovaj dodatak trenutno podržava samo rad preko komandne linije. "
"Izvršite...\n"
"\t\t\t<pre>php tools/importExport.php CSVImportExportPlugin</pre>\n"
"\t\t\t...za više informacija.</p>\n"
"\t"

msgid "plugins.importexport.csv.cliUsage"
msgstr ""
"Alatka komandne linije za uvoz CSV podataka u OMP\n"
"\t\t\tUpotreba:\n"
"\t\t\t{$scriptName} [--dry-run] fileName.csv korisničko_ime\n"
"\t\t\tOpcija --dry-run se može koristiti za testiranje bez pravljenja promjena.\n"
"\t\t\tNavedite korisničko ime kojem želite da dodijelite podneske.\n"

msgid "plugins.importexport.csv.fileDoesNotExist"
msgstr "Datoteka \"{$filename}\" ne postoji. Izlazim."

msgid "plugins.importexport.csv.unknownUser"
msgstr "Nepoznat korisnik: \"{$username}\". Izlazim."

msgid "plugins.importexport.csv.unknownLocale"
msgstr "Nepoznat lokal: \"{$locale}\". Preskačem."

msgid "plugins.importexport.csv.unknownPress"
msgstr "Nepoznata izdavačka kuća: \"{$contextPath}\". Preskačem."

msgid "plugins.importexport.csv.noGenre"
msgstr "Ne postoji žanr rukopisa. Izlazim."

msgid "plugins.importexport.csv.noAuthorGroup"
msgstr ""
"Ne postoji podrazumijevana grupa autora u izdavačkoj kući {$press}. "
"Preskačem ovaj podnesak."

msgid "plugins.importexport.csv.noSeries"
msgstr ""
"Putnja serije {$seriesPath} ne postoji. Nije moguće dodati je ovom podnesku."

msgid "plugins.importexport.csv.import.submission"
msgstr "Podnesak: '{$title}' je uspješno uvezen."

msgid "plugins.importexport.csv.import.success.description"
msgstr ""
"Uvoz je bio uspješan. Uspješno uvezene stavke su navedene ispod."

msgid "plugins.importexport.csv.invalidHeader"
msgstr "CSV datoteka ili nema zaglavlje ili ima neispravno zaglavlje, pogledajte primjer datoteke \"sample.csv\" koja se nalazi u folderu dodatka."

msgid "plugins.importexport.csv.invalidAuthor"
msgstr "Autor \"{$author}\" ima neispravan format i preskočen je."
